<div class="row">
    <div class="col-md-4">

<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), html: {class: "well"}) do |f| %>
  <%= devise_error_messages! %>
  <fieldset>
      <legend><h2>Sign Up</h2></legend>
  <div><%= f.input :first_name, :input_html => {:size => 30}%><br />
  </div>
  	 
  <div><%= f.input :last_name, :input_html => {:size => 30}%><br />
 </div>

  <div><%= f.input :profile_name, :input_html => {:size => 30}%><br />
  </div>

  <div><%= f.input :email, :input_html => {:size => 30} %> <br /> </div>

  <div><%= f.input :password, :input_html => {:size => 30} %> <br />
   </div>

  <div><%= f.input :password_confirmation, :input_html => {:size => 30} %><br /></div>

  <div><%= f.button :submit, "Sign up" %></div>
<% end %>
</fieldset>
<%= render "devise/shared/links" %>
 </div>
  </div>