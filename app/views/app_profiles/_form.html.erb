<%= simple_form_for(@app_profile) do |f| %>
  <%= f.error_notification %>

  <div class="row">
  <div class="col-md-3">
      <%= f.input :name, label: false, placeholder: "App Name", input_html: { class: "form-control"} %>
  </div>
</div>

 <div class="form-actions"
    <%= f.button :submit %>
  </div>
<% end %> 

