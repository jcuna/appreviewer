<div class="container">
<div class="jumbotron">
<h1 align="center">Listing All Apps.</h1>
</div>


<% @app_profiles.each do |app_profile| %>

<div class="status">
  <div class="row">
    <div class="col-sm-12 col-md-6">
      
    <p><%= app_profile.name %></p>
      <div class="meta">
          | <%= link_to "Edit", edit_app_profile_path(app_profile) %> |
          <%= link_to "Delete", app_profile, method: :delete, data: { confirm: "Are you sure you want to delete this app?"} %>
      </div>
    </div>
  </div>
</div>
<% end %>
</div>