<h1>Listing app_profiles</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @app_profiles.each do |app_profile| %>
  <tr>
    <td><%= app_profile.name %></td>
    <td><%= link_to 'Show', app_profile %></td>
    <td><%= link_to 'Edit', edit_app_profile_path(app_profile) %></td>
    <td><%= link_to 'Destroy', app_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New App profile', new_app_profile_path %>
